  "devDependencies": {
    "coveralls": "^3.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "jest-fetch-mock": "^1.6.6",
    "react-test-renderer": "^16.5.2",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.3.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
